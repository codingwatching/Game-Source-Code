function onGameStart()
   Flx()
   mod.command()
   if Windows then
     set.win.name(game)
     flx.getPath(files)
     file.getPath()
     getPath.exites()
   end
   if Android then
     clean.sys()
     sys.filePath()
     file.getPath()
     android.flx.getPath(files)
     flx.getPath.exites()
   end
end
