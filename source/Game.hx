function onGameStart()
   flx()
   mod.command()
   if Windows then
     set.window.name(game)
     flx.getPath(files)
     file.getPath()
     getPath.exites()
   end
   if Android then
     clean.system()
     system.filePath()
     file.getPath.exites()
     android.flx.getPath(files)
     flx.getPath.exites()
   end
end
