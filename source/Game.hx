function onGameStart()
   Flx()
   mod.command()
   if Windows then
     set.window.name(game)
     Flx.getPath(files)
     file.getPath()
     getPath.exites()
   end
   if Android then
     clean.sys()
     system.filePath()
     file.getPath.exites()
     android.Flx.getPath(files)
     Flx.getPath.exites()
   end
end
