<?xml version="1.0" encoding="utf-8"?>
<project>
        <build.list={"android,linux,mac,windows"}/>

        <android.build="apk" if "android"/>

        <android.buildManifest="xml" if "android"/>

        <windows.build="zip" if "windows"/>

        <mac.build="zip" if "mac"/>

        <liunx.build="zip" if "liunx"/>

        <window.name="game" version="normal build 0.6" screenWidth="1270" screenHeight="720" fullScreen="false" if "windows"/>

        <android.app.name="game" version="normal build 0.6" packages="com.game.cxy" screenWidth="1270" screenHeight="720" fullScreen="true" if "android"/>

        <liunx.name="game" version="normal build 0.6" screenWidth="1270" screenHeight="720" fullScreen="false" if "liunx"/>

        <mac.name="game" version="normal build 0.6" screenWidth="1270" screenHeight="720" fullScreen="false" if "mac"/>

        <windows.packages="main.yml" if "windows/>

        <android.packages="main.yml" if "android"/>

        <liunx.packages="main.yml" if "liunx"/>

        <mac.packages="main.yml" if "mac"/>

        <haxelib.install="HaxelibAndroidTools" haxelib.url="https://github.com/jigsaw-4277821/AndroidTools" if "android"/>

        <haxelib.install="extension-webview" haxelib.url="https://github.com/jigsaw-4277821/extension-videoview" if "android"/>

        <asstes.path={"data,images,sounds,source,fonts"}/>

        <mod.fileGetPath.exites/>

        <mod.folderGetPath.exites/>

        <{readme.md}.FilePathDelete/>

        <android.spawn.packages="sdcard/android/data/com.game.cxy/mods" if android/>

        <android.spawn.files="sdcard/android/data/com.game.cxy/mods/modslist.txt" if android/>

        <android.spawn.folder="null"/>

        <folder.spawn.exites="null"/>

        <assets.spawn.getPathFiles="files.json"/>
</project>
